//C++ program to find first position of the given key in the given array of size n
//Using Linear Search

#include<bits/stdc++.h>
using namespace std;
int main()
{
    int n;
    cin>>n;
    int arr[n];
    for(int i=0;i<n;i++)
        cin>>arr[i];
    for(int i=0;i<n;i++)
    {
        if(key==arr[i])
        cout<<i<<endl;
        break;
    }
return 0;
}
